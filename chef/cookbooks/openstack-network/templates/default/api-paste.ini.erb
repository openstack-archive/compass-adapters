<%= node["openstack"]["network"]["custom_template_banner"] %>

[composite:neutron]
use = egg:Paste#urlmap
/: neutronversions
/v2.0: neutronapi_v2_0

[composite:neutronapi_v2_0]
use = call:neutron.auth:pipeline_factory
noauth = <%= node['openstack']['network']['neutron-server']['compoiste']['neutronapi_v2_0']['noauth'].join(' ') %>
keystone = <%= node['openstack']['network']['neutron-server']['compoiste']['neutronapi_v2_0']['keystone'].join(' ') %>

<% if node['openstack']['network']['neutron-server']['filter']['request_id'] -%>
[filter:request_id]
paste.filter_factory = <%= node['openstack']['network']['neutron-server']['filter']['request_id'] %>
<% end -%>

<% if node['openstack']['network']['neutron-server']['filter']['catch_errors'] -%>
[filter:catch_errors]
paste.filter_factory = <%= node['openstack']['network']['neutron-server']['filter']['catch_errors'] %>
<% end -%>

<% if node['openstack']['network']['neutron-server']['filter']['access_log'] -%>
[filter:access_log]
paste.filter_factory = <%= node['openstack']['network']['neutron-server']['filter']['access_log'] %>
<% end -%>

[filter:keystonecontext]
paste.filter_factory = neutron.auth:NeutronKeystoneContext.factory

[filter:extensions]
paste.filter_factory = neutron.api.extensions:plugin_aware_extension_middleware_factory

[app:neutronversions]
paste.app_factory = neutron.api.versions:Versions.factory

[app:neutronapiapp_v2_0]
paste.app_factory = neutron.api.v2.router:APIRouter.factory

[filter:authtoken]
paste.filter_factory = keystoneclient.middleware.auth_token:filter_factory
<% if node['openstack']['network']['delay_auth_decision'] -%>
delay_auth_decision = <%= node['openstack']['network']['delay_auth_decision'] %>
<% end -%>
